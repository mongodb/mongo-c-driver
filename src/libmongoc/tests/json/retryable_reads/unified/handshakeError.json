            "client": "client0",
            "failPoint": {
              "configureFailPoint": "failCommand",
              "mode": {
                "times": 2
              },
              "data": {
                "failCommands": [
                  "saslContinue",
                  "ping"
                ],
                "closeConnection": true
              }
            }
          }
        },
        {
          "name": "runCommand",
          "object": "database0",
          "arguments": {
            "commandName": "ping",
            "command": {
              "ping": 1
            }
          },
          "expectError": {
            "isError": true
          }
        },
        {
          "name": "find",
          "object": "collection0",
          "arguments": {
            "filter": {
              "_id": 2
            }
          },
          "expectResult": [
            {
              "_id": 2,
              "x": 22
            }
          ]
